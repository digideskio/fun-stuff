{
  "manifest_version": 2,
  "name"            : "Glenn's Enhancements",
  "version"         : "1.0",
  "description"     : "My first Chrome extension",
  "browser_action":{
    "default_icon"  : "icon.png",
    "default_popup" :"popup.html"
  },
  "content_scripts":[{
    "js"      : ["jquery.js","background.js"],
    "matches" : ["http://*/*", "https://*/*"]
    }]
}